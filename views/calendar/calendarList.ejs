<h1>월별 계획</h1>
<table class="table table-sm table-bordered" style="width: 600px">
    <th>Month</th>
    <th>Plan</th>
    <th>수정</th>
    <th>삭제</th>
    <%
        var i = 0;

        while(i < results.length) {
    %>
        <tr>
            <td class="td td-sm"><%=results[i].title%></td>
            <td><%=results[i].description%></td>
            <td><a href="/calendar/update/<%=results[i].id%>">update</a></td>
            <td><a href="/calendar/delete_process/<%=results[i].id%>">delete</a></td>
        </tr>
    <%
        i++;
        }
    %>
</table>