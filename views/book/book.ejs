<h1><%=kind%></h1>
<table class="table table-sm table-bordered" style="width: 650px">
    <th>책 정보</th> <th>제목</th> <th>저자</th> <th>가격</th>
    <%
        var i = 0;

        while(i < results.length) {
    %>
            <tr>
                <td><img src="/images/<%=results[i].img%>" style="width:90px;height:130px; "></td>
                <td><a href="/book_detail/<%=results[i].id%>"><%=results[i].name%></a></td>
                <td><%=results[i].author%></td>
                <td><%=results[i].price%>원</td>
            </tr>
    <%
            i++;
        }
    %>
</table>